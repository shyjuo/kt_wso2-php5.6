<project>
    <pomVersion>3</pomVersion>
    <id>org.wso2.javascript.rhino</id>
    <name>JavaScript Webservices</name>
    <currentVersion>${js.webservices.version}</currentVersion>

   <!--                                      -->
   <!-- Project management section           -->
   <!--                                      -->
   
   <!--                                      -->
   <!-- Project dependency section           -->
   <!--                                      -->    
    <dependencies>
	<dependency>
            <groupId>org.mozilla.javascript</groupId>
            <artifactId>js-core</artifactId>
            <version>${js.core.version}</version>
        </dependency>
        <dependency>
            <groupId>stax</groupId>
            <artifactId>stax-api</artifactId>
            <version>${stax.api.version}</version>
        </dependency>
        <dependency>
            <groupId>${stax.impl.groupid}</groupId>
            <artifactId>${stax.impl.artifactid}</artifactId>
            <version>${stax.impl.version}</version>
        </dependency>        
        <dependency>
            <groupId>org.apache.ws.commons.axiom</groupId>
            <artifactId>axiom-api</artifactId>
            <version>${axiom.version}</version>
        </dependency>
        <dependency>
            <groupId>org.apache.ws.commons.axiom</groupId>
            <artifactId>axiom-impl</artifactId>
            <version>${axiom.version}</version>
        </dependency>
        <dependency>
            <groupId>commons-logging</groupId>
            <artifactId>commons-logging</artifactId>
            <version>${commons.logging.version}</version>
            <properties>
                <module>true</module>
            </properties>
        </dependency>
    </dependencies>

   <!--                                      -->
   <!-- Project build and reports section    -->
   <!--                                      -->    
    <build>
        <nagEmailAddress></nagEmailAddress>
        <sourceDirectory>src</sourceDirectory>
    </build>

</project>

